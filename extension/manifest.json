{
    "manifest_version": 3,
    "name": "YouTube Embed Switcher",
    "version": "1.0",
    "description": "Switch YouTube URLs to embed URLs.",
    "permissions": ["tabs"],
    "host_permissions": ["*://*.youtube.com/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "Switch to Embed"
    }
  }
  